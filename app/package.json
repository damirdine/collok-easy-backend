{
  "type": "module",
  "name": "api_collokeasy",
  "version": "1.0.0",
  "description": "api-express",
  "main": "app.js",
  "scripts": {
    "build": "rm -r dist/* && tsc",
    "test": "node --env-file=.env.test --experimental-vm-modules ./node_modules/.bin/jest",
    "test:watch": "node --env-file=.env.test --experimental-vm-modules ./node_modules/.bin/jest --watchAll",
    "start": "node app.js",
    "start:watch": "nodemon -L app.js",
    "docker:api": "docker exec -it api sh",
    "dev": "ts-node-dev --respawn --transpile-only app.ts",
    "db:up": "npx sequelize-cli db:migrate --debug && npx sequelize-cli db:seed:all --debug",
    "typecheck": "tsc --noEmit"
  },
  "author": "CollokEasy",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "argon2": "^0.31.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.2",
    "nodemailer": "^6.9.8",
    "nodemon": "^3.0.1",
    "sequelize": "^6.35.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
